{
  "author": "Barak Binyamin",
  "description": "A cross platform desktop app for scraping music using Google, Genius.com, & Youtube",
  "name": "kiot-dl",
  "license": "ISC",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "prep"   : "npm install && cd view && npm install",
    "dev"    : "env DEV=true electron-forge start",
    "start"  : "cd view && npm run build && cd .. && electron-forge start",
    "package": "cd view && npm run build && cd .. && electron-forge package",
    "make"   : "cd view && npm run build && cd .. && electron-forge make",
    "clean"  : "bash clean.sh"
  },
  "config": {
    "forge": "./electron/forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "electron": "^28.1.1"
  },
  "dependencies": {
    "@electron-forge/maker-dmg": "^7.2.0",
    "axios": "^1.6.5",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "find-free-port": "^2.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fuse.js": "^7.0.0",
    "http": "^0.0.1-security",
    "jsdom": "^23.2.0",
    "node-json-db": "^2.3.0",
    "progress-stream": "^2.0.0",
    "socket.io": "^4.7.3",
    "uuid": "^9.0.1",
    "ytdl-core": "^4.11.5"
  }
}
